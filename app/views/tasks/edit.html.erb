<h1>Edit your Task</h1>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed? %>
  <%= f.check_box :completed %>
  <%= f.submit %>
<% end %>

<p> <%= link_to "All tasks", tasks_path %> </p>
<p> <%= link_to "Create new task", create_path %> </p>
